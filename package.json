    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^25.1.0",
    "concat-stream": "^2.0.0",
    "dotenv-override-true": "^6.2.0",
    "form-data": "^3.0.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "pubnub": "^4.27.3",
    "ramda": "^0.27.0",
    "standard": "^14.3.1",
    "wait-for-async": "^0.5.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "yarn-upgrade-all": "^0.5.2"
